<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="filter_l10n_ua" model="ir.filters">
        <field name="name">UA Localization</field>
        <field name="model_id">ir.module.module</field>
        <field name="user_id" eval="False"/>
        <field name="domain">[("name", "like", "l10n_ua")]</field>
        <field name="context">{'group_by': []}</field>
        <field name="sort">["name asc"]</field>
        <field name="action_id" ref="base.open_module_tree"/>
        <field name="is_default" eval="False"/>
    </record>

</odoo>
